# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/src/Exoterra/[Ll]ibrary/
/src/Exoterra/[Tt]emp/
/src/Exoterra/[Oo]bj/
/src/Exoterra/[Bb]uild/
/src/Exoterra/[Bb]uilds/
/src/Exoterra/[Ll]ogs/
/src/Exoterra/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/src/Exoterra/[Mm]emoryCaptures/

# Recordings can get excessive in size
/src/Exoterra/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/src/Exoterra/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/src/Exoterra/.vs/

# Gradle cache directory
/src/Exoterra/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/src/Exoterra/ExportedObj/
/src/Exoterra/.consulo/
/src/Exoterra/*.csproj
/src/Exoterra/*.unityproj
/src/Exoterra/*.sln
/src/Exoterra/*.suo
/src/Exoterra/*.tmp
/src/Exoterra/*.user
/src/Exoterra/*.userprefs
/src/Exoterra/*.pidb
/src/Exoterra/*.booproj
/src/Exoterra/*.svd
/src/Exoterra/*.pdb
/src/Exoterra/*.mdb
/src/Exoterra/*.opendb
/src/Exoterra/*.VC.db

# Unity3D generated meta files
/src/Exoterra/*.pidb.meta
/src/Exoterra/*.pdb.meta
/src/Exoterra/*.mdb.meta

# Unity3D generated file on crash reports
/src/Exoterra/sysinfo.txt

# Builds
/src/Exoterra/*.apk
/src/Exoterra/*.aab
/src/Exoterra/*.unitypackage
/src/Exoterra/*.unitypackage.meta
/src/Exoterra/*.app

# Crashlytics generated file
/src/Exoterra/crashlytics-build.properties

# Packed Addressables
/src/Exoterra/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/src/Exoterra/[Aa]ssets/[Ss]treamingAssets/aa.meta
/src/Exoterra/[Aa]ssets/[Ss]treamingAssets/aa/*
